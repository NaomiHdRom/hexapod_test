  <xacro:macro name="leg" params="prefix  
  
                                          origin_xA origin_yA origin_zA                                 
                                          origin_xB origin_yB origin_zB                                        
                                          origin_xC origin_yC origin_zC
                                          origin_xP origin_yP origin_zP

                                          ixx_A  ixy_A   ixz_A   iyy_A  iyz_A  izz_A
                                          ixx_B  ixy_B   ixz_B   iyy_B  iyz_B  izz_B
                                          ixx_C  ixy_C   ixz_C   iyy_C  iyz_C  izz_C
                                          ixx_P  ixy_P   ixz_P   iyy_P  iyz_P  izz_P
                                    
                                          joint_xA joint_yA joint_zA 
                                          joint_rA joint_pA joint_jA

                                          joint_xB joint_yB joint_zB 
                                          joint_rB joint_pB joint_jB 

                                          joint_xC joint_yC joint_zC
                                          joint_rC joint_pC joint_jC  

                                          joint_xP joint_yP joint_zP
                                          joint_rP joint_pP joint_jP 

                                          axisAx axisBx axisCx  axisPx
                                          axisAy axisBy axisCy  axisPy                             
                                          axisAz axisBz axisCz  axisPz
                                          

                                          stlA_path stlB_path stlC_path   stlP_path

                                          lowerA lowerB lowerC
                                          upperA upperB upperC

                                          mat_name    
                                        "                                     >

    <link name="${prefix}_linkA">
      <inertial>
        <mass value="${mass_A}"/>
        <origin xyz="${origin_xA} ${origin_yA} ${origin_zA}" rpy="0 0 0"/>
        <inertia ixx="${ixx_A}" ixy="${ixy_A}" ixz="${ixz_A}"
                 iyy="${iyy_A}" iyz="${iyz_A}"
                 izz="${izz_A}"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="${stlA_path}" />
        </geometry>
    
        <material name="${mat_name}"/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="${stlA_path}" />
        </geometry>
      </collision>
    </link>

    <joint name="${prefix}_jointA" type="revolute">
      <origin xyz="${joint_xA} ${joint_yA} ${joint_zA}" rpy="${joint_rA} ${joint_pA} ${joint_jA}"/>
      <parent link="base_link"/>
      <child link="${prefix}_linkA"/>
      <axis xyz="${axisAx} ${axisAy} ${axisAz}"/>
      <limit  lower="${lowerA}" upper="${upperA}" effort="10" velocity="1.0"/>
      <dynamics damping="0.5" friction="0.1"/>
    </joint>

    <link name="${prefix}_linkB">
      <inertial>
        <mass value="${mass_B}"/>
        <origin xyz="${origin_xB} ${origin_yB} ${origin_zB}" rpy="0 0 0"/>
        <inertia ixx="${ixx_B}" ixy="${ixy_B}" ixz="${ixz_B}"
                 iyy="${iyy_B}" iyz="${iyz_B}"
                 izz="${izz_B}"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="${stlB_path}" />
        </geometry>
      
        <material name="${mat_name}"/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="${stlB_path}" />
        </geometry>
      </collision>
    </link>

    <joint name="${prefix}_jointB" type="revolute">
      <origin xyz="${joint_xB} ${joint_yB} ${joint_zB}" rpy="${joint_rB} ${joint_pB} ${joint_jB}"/>
      <parent link="${prefix}_linkA"/>
      <child link="${prefix}_linkB"/>
      <axis xyz="${axisBx} ${axisBy} ${axisBz}"/>
      <limit  lower="${lowerB}" upper="${upperB}" effort="10" velocity="1.0"/>
      <dynamics damping="0.5" friction="0.1"/>
    </joint>

    <link name="${prefix}_linkC">
      <inertial>
        <mass value="${mass_C}"/>
        <origin xyz="${origin_xC} ${origin_yC} ${origin_zC}" rpy="0 0 0"/>
        <inertia ixx="${ixx_C}" ixy="${ixy_C}" ixz="${ixz_C}"
                 iyy="${iyy_C}" iyz="${iyz_C}"
                 izz="${izz_C}"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="${stlC_path}" />
        </geometry>
   
        <material name="${mat_name}"/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="${stlC_path}" />
        </geometry>
      </collision>
    </link>

    <joint name="${prefix}_jointC" type="revolute">
      <origin xyz="${joint_xC} ${joint_yC} ${joint_zC}" rpy="${joint_rC} ${joint_pC} ${joint_jC}"/>
      <parent link="${prefix}_linkB"/>
      <child link="${prefix}_linkC"/>
      <axis xyz="${axisCx} ${axisCy} ${axisCz}"/>
      <limit  lower="${lowerC}" upper="${upperC}" effort="10" velocity="1.0"/>
      <dynamics damping="0.5" friction="0.1"/>
    </joint>

    <link name="${prefix}_linkP">
      <inertial>
        <mass value="0.001"/>
        <origin xyz="${origin_xP} ${origin_yP} ${origin_zP}" rpy="0 0 0"/>
        <inertia ixx="${ixx_P}" ixy="${ixx_P}" ixz="${ixz_P}"
                 iyy="${iyy_P}" iyz="${iyz_P}"
                 izz="${izz_P}"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="${stlP_path}" />
        </geometry>

        <material name="${mat_name}"/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="${stlP_path}" />
        </geometry>
      </collision>
    </link>

    <joint name="${prefix}_jointP" type="fixed">
      <origin xyz="${joint_xP} ${joint_yP} ${joint_zP}" rpy="${joint_rP} ${joint_pP} ${joint_jP}"/>
      <parent link="${prefix}_linkC"/>
      <child link="${prefix}_linkP"/>
      <axis xyz="${axisPx} ${axisPy} ${axisPz}"/>
    </joint>

  </xacro:macro>


